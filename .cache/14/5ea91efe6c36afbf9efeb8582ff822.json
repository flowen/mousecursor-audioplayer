constructor(w, h) {\n    this.opacity = 0\n    this.speed = 2.5\n    this.pos = {\n      x: w / 2,\n      y: h / 2,\n    }\n    this.vel = {\n      x: 0,\n      y: 0,\n    }\n    this.acc = 2\n    this.delta = 0.01\n    // this.rotation = 0\n    this.radius = 50\n    this.force = 2\n    this.audio = ''\n  }\n\n  // set _rotation(angle) {\n  //   this.rotation = angle\n  // }\n\n  get _getPos() {\n    return this.pos\n  }\n\n  get _getOpacity() {\n    return this.opacity\n  }\n\n  fadeIn() {\n    this.opacity < 1 ? (this.opacity += this.delta) : (this.opacity = 1)\n  }\n\n  fadeOut() {\n    this.opacity > 0 ? (this.opacity -= this.delta * 2) : (this.opacity = 0)\n  }\n\n  follow(mouseCoords) {\n    // get the distance (hypotenuse) and calculate the normal\n    // add this somehow to accelerate (how?) with math.abs!\n    const distance = VMath.distance(mouseCoords, this.pos)\n    const normal = VMath.normalize(distance)\n\n    const angle = VMath.angle(mouseCoords, this.pos)\n\n    if (distance.x > 2 || distance.x < -2) {\n      this.vel.x = Math.cos(angle) * this.speed\n      this.pos.x += this.vel.x\n    }\n\n    if (distance.y > 2 || distance.y < -2) {\n      this.vel.y = Math.sin(angle) * this.speed\n      this.pos.y += this.vel.y\n    }\n\n    // this.rotation = angle\n  }\n\n  loadPlayer() {\n    initPlayer()\n  }\n\n  draw(ctx, mouseOver) {\n    !mouseOver ? this.fadeOut() : this.fadeIn()\n\n    ctx.save()\n    ctx.translate(this.pos.x, this.pos.y)\n    // ctx.rotate(this.rotation)\n    ctx.beginPath()\n    ctx.fillStyle = `rgba(114, 127, 187, ${this.opacity})`\n    ctx.arc(0, 0, this.radius, 0, Math.PI * 2)\n    ctx.fill()\n    ctx.restore()\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"f47342ad02af424a7e006ed06935499c","cacheData":{"env":{}}}