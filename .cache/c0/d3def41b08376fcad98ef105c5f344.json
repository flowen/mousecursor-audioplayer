ionFrame(draw)\n}\n\nconst draw = () => {\n  clearCanvas()\n\n  if (mouseOver) {\n    cursor.fadeIn()\n  } else if (cursor._getOpacity > 0) {\n    cursor.fadeOut()\n  }\n\n  cursor.follow(mouseCoords)\n  cursor.draw(ctx, mouseOver)\n\n  requestAnimationFrame(draw)\n}\n\ninit()\n"},"lineCount":null}},"error":null,"hash":"058f8fae3d5e247cdb9bbc70f1532121","cacheData":{"env":{}}}